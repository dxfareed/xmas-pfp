generator client {
  provider = "prisma-client-js"
}

generator scripts {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model UserNFTs {
  ownerFid  BigInt   @id
  nfts      Json
  updatedAt DateTime @updatedAt
}
